{
  "name": "ngx-ionic-image-viewer-workspace",
  "version": "0.0.0",
  "description": "The angular workspace to develop the ngx-ionic-image-viewer component",
  "keywords": [
    "ionic",
    "ionic4",
    "angular",
    "angular8",
    "ngx",
    "module",
    "component",
    "directive",
    "ion",
    "image",
    "img",
    "photo",
    "thumbnail",
    "view",
    "viewer",
    "show",
    "gallery",
    "zoom",
    "modal",
    "slider"
  ],
  "homepage": "https://github.com/herdwatch-apps/ngx-ionic-image-viewer.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/herdwatch-apps/ngx-ionic-image-viewer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Thomas Simon",
    "email": "t.simon@live.com"
  },
  "contributors": [
    {
      "name": "Maxim Belov",
      "email": "belov1988@gmail.com",
      "url": "https://github.com/MaximBelov"
    }
  ],
  "scripts": {
    "ng": "ng",
    "start": "nx serve --project demo",
    "prebuild": "cp README.md libs/ngx-ionic-image-viewer/ ",
    "build": "nx run-many --target=build --configuration production --all",
    "build:watch": "nx build --project ngx-ionic-image-viewer --configuration production --watch",
    "release-silent": "dotenv -- bash -c \"cd libs/ngx-ionic-image-viewer && release-it --ci\"",
    "release": "dotenv -- bash -c \"cd libs/ngx-ionic-image-viewer && release-it\"",
    "release-major": "dotenv -- bash -c \"cd libs/ngx-ionic-image-viewer && release-it major\"",
    "release:dry-run": "dotenv -- bash -c \"cd libs/ngx-ionic-image-viewer && release-it --ci --dry-run\"",
    "test": "nx run-many --target=test --configuration production --all --skip-nx-cache",
    "lint": "nx lint --project ngx-ionic-image-viewer --verbose",
    "e2e": "nx e2e demo-e2e --configuration=coverage",
    "e2e:open": "nx e2e demo-e2e --configuration=coverage --watch --skip-nx-cache"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@herdwatch/ngx-ionic-image-viewer": "^2.1.8",
    "globals": "^15.11.0",
    "swiper": "^11"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.10",
    "@angular-devkit/core": "18.2.10",
    "@angular-devkit/schematics": "18.2.10",
    "@angular-eslint/eslint-plugin": "18.2.0",
    "@angular-eslint/eslint-plugin-template": "18.2.0",
    "@angular-eslint/template-parser": "18.2.0",
    "@angular/cli": "18.2.10",
    "@angular/compiler-cli": "18.2.9",
    "@angular/language-service": "18.2.9",
    "@awesome-cordova-plugins/core": "^6.9.0",
    "@awesome-cordova-plugins/splash-screen": "6.9.0",
    "@awesome-cordova-plugins/status-bar": "6.9.0",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@cypress/code-coverage": "^3.13.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@nx/angular": "19.8.8",
    "@nx/cypress": "19.8.8",
    "@nx/eslint": "19.8.8",
    "@nx/eslint-plugin": "^20.0.6",
    "@nx/jest": "19.8.8",
    "@nx/js": "19.8.8",
    "@nx/linter": "19.8.4",
    "@nx/web": "19.8.8",
    "@nx/workspace": "19.8.8",
    "@release-it/conventional-changelog": "^9.0.1",
    "@schematics/angular": "^18.2.10",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/jest": "^29.5.14",
    "@types/node": "20.17.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "cypress": "^13.15.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "4.0.0",
    "husky": "^9.1.6",
    "istanbul-lib-coverage": "^3.2.2",
    "jasmine-core": "~5.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~14.2.4",
    "ng-mocks": "^14.13.1",
    "ng-packagr": "^18.2.1",
    "ngx-build-plus": "^18.0.0",
    "nx": "18.2.4",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2"
  }
}
